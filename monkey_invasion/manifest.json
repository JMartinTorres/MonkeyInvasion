{
  "manifest_version": 2,
  "name": "Monkey Invasion",
  "short_name": "MonkeyInvasion",
  "version": "1.0.0.0",
  
  "description": "",
  "icons": {
    "16": "extension_icons/youtubeplaylistfirstvideo-16.png",
    "32": "extension_icons/youtubeplaylistfirstvideo-32.png",
    "48": "extension_icons/youtubeplaylistfirstvideo-48.png",
    "128": "extension_icons/youtubeplaylistfirstvideo-128.png"
  },
 
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery/jquery-3.4.1.min.js",
        "content_scripts/they_re_here.js"
      ]
    }
  ],
  
  "permissions": [
    "tabs",
    "activeTab"
  ],
  
  "web_accessible_resources": [
	"content_scripts/makemonkeysgo.css",
	"monkeys/*",
	"mewsick/*"
	]
}
